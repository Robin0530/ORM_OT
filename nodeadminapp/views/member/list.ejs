    <!--우측 상단 헤더 영역 -->
    <div class="row wrapper border-bottom white-bg page-heading">
        <div class="col-lg-10">
            <h2>사용자 정보관리</h2>
            <ol class="breadcrumb">
                <li>
                    <a href="/">Home ></a>
                </li>
                <li>
                    <a>사용자정보관리 ></a>
                </li>
                <li class="active">
                    <strong>조회</strong>
                </li>
            </ol>
        </div>
        <div class="col-lg-2">

        </div>
    </div>

    <!--우측 콘텐츠 영역 -->
    <div class="wrapper wrapper-content">

        <!-- 사용자 정보조회 옵션 및 조회 영역 -->
        <form action="/member/list" method="post">
            <div class="ibox-content m-b-sm border-bottom">
                <div class="row">
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label class="control-label" for="OrderNo">메일주소</label>
                            <input type="text" name="email" class="form-control">
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label class="control-label" for="OrderName">이름</label>
                            <input type="text" name="name" class="form-control">
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label class="control-label" for="MobileNo">전화번호</label>
                            <input type="tel" name="telephone" class="form-control">
                        </div>
                    </div>
                </div>
                <div class="text-center">
                    <button type="submit" class="btn btn-primary">조회</button>
                    <a href="/member/entry" class="btn btn-info">신규</a>
                </div>
            </div>
        </form>

        <!-- 사용자 정보조회 결과 목록 표시 영역 -->
        <div class="row">
            <div class="col-lg-12">
                <div class="ibox float-e-margins">
                    <div class="ibox-content">
                        <div class="table-responsive">
                            <table class="table table-hover">
                                <thead>
                                    <tr>
                                        <th width="10%">순번</th>
                                        <th width="20%">메일주소</th>
                                        <th width="10%">이름</th>
                                        <th width="20%">전화번호</th>
                                        <th width="10%">생년월일</th>
                                        <th width="10%">가입상태</th>
                                        <th width="20%">등록일자</th>
                                    </tr>
                                </thead>
                                <tbody class="hoverTblBody">
                                    <% for(var i=0; i < member.length; i++) { %>
                                    <tr>
                                        <td><a href="/member/modify/<%=member[i].member_id%>"></a><%=member[i].member_id%></td>
                                        <td><%=member[i].email%></td>
                                        <td><%=member[i].name%></td>
                                        <td><%=member[i].telephone%></td>
                                        <td><%=member[i].birth_date%></td>
                                        <td><%=member[i].use_state_code%></td>
                                        <td><%=member[i].reg_date%></td>
                                    </tr>
                                    <% } %>
                                </tbody>
                            </table>

                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>

    <script>
    </script>