    <!--우측 콘텐츠 영역 -->
    <div class="wrapper wrapper-content">
        <!-- 동기방식 -->
        <!-- <form action="/member/syncentry" method="post"> -->

        <!-- 비동기방식 -->
        <form action="/member/entry" method="post">
            <div class="ibox-content m-b-sm border-bottom">
                <!-- 첫번째 row -->
                <div class="row">
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label class="control-label">메일주소</label>
                            <input type="text" name="email" class="form-control">
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label class="control-label">비밀번호</label>
                            <input type="member_password" name="member_password" class="form-control">
                        </div>
                    </div>

                    <div class="col-sm-4">
                        <div class="form-group">
                            <label class="control-label">이름</label>
                            <input type="text" name="name" class="form-control">
                        </div>
                    </div>
                </div>

                <!-- 두번째 row -->
                <div class="row">
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label class="control-label">사진업로드</label>
                            <input type="file" name="file" class="form-control">
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label class="control-label">전화번호</label>
                            <input type="text" name="telephone" class="form-control">
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label class="control-label">생년월일</label>
                            <input type="text" name="birth_date" class="form-control" placeholder="19910530 방식으로 입력해주세요">
                        </div>
                    </div>
                </div>

                <!-- 세번째 row -->
                <div class="row">
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label class="control-label">회원가입유형</label>
                            <select name="entry_type_code" class="form-control">
                                <option value="0">직접가입</option>
                                <option value="1">SNS-Facebook</option>
                                <option value="2">SNS-Google</option>
                                <option value="3">SNS-Kakao</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label class="control-label">가입상태정보</label>
                            <select name="use_state_code" class="form-control">
                                <option value="0">일시중지</option>
                                <option value="1" selected>사용중</option>
                                <option value="2">탈퇴상태</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-sm-4">
                        <div class="form-group">
                            <label class="control-label">가입일시</label>
                            <input type="text" name="reg_date" class="form-control" readonly>
                        </div>
                    </div>
                </div>

                <div class="text-center">
                    <button type="submit" class="btn btn-primary">저장</button>
                    <a href="/member/list" class="btn btn-info">목록</a>
                </div>
            </div>
        </form>
    </div>

